allowed_patterns:
  - 'GH_CLIENT_SECRET: \${{ secrets.GH_OAUTH_CLIENT_SECRET }}'
  - 'GH_CLIENT_SECRET: \${{ secrets.GH_OAUTH_CLIENT_SECRET }}'
  - 'AUTH_GITHUB_CLIENT_SECRET: {{ .Values.GH_CLIENT_SECRET \| b64enc }}'
  - 'password: \${'
  - 'clientSecret: \${GH_CLIENT_SECRET}'
  - 'REPO_SECRETS: \${{ toJson\(secrets\) }}'
  - 'key: cypress\-cache\-v2\-\${{ runner.os }}\-'
  - 'key: \${{ runner.os }}-'
  - 'secret_name=\$OPTARG'
  - 'pwd=\$OPTARG'
  - 'key_value_literals="\$\{key_value_literals\} --from-literal=\$\{OPTARG\}"'
  - '-k    key-value pair, i.e. foo=bar'

fileignoreconfig:
  - filename: yarn.lock
    checksum: d074a22bc00b818a9ff80c3f5c4460983cf63e07b7e75e1ac8e98b2cc0f4a2c1
